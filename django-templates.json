{
  "static_paths": ["DjangoTemplates/static"],
  "template_paths": ["DjangoTemplates/templates"],
  "contexts_path": "DjangoTemplates",
  "pages": [
    {
      "name": "Home page",
      "contexts": ["context.json"],
      "template": "mentor_template/index.html"
    },
    {
      "name": "About",
      "contexts": ["base.json", "about.json"],
      "template": "mentor_template/about.html"
    },
    {
      "name": "Contact",
      "contexts": ["base.json", "contact.json"],
      "template": "mentor_template/contact.html"
    },
    {
      "name": "Courses",
      "contexts": ["base.json", "courses.json"],
      "template": "mentor_template/courses.html"
    },
    {
      "name": "Pricing",
      "contexts": ["base.json", "pricing.json"],
      "template": "mentor_template/pricing.html"
    },
    {
      "name": "Course Details",
      "contexts": ["base.json", "course-details.json"],
      "template": "mentor_template/course-details.html"
    }
  ]
}
